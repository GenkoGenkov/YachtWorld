@model IEnumerable<YachtServiceModel>

@{
    ViewBag.Title = "My Yachts";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (Model.Any())
{
    <div class="row">
        @foreach (var yacht in Model)
        {
            <partial name="_YachtPartial" model="@yacht" />
        }
    </div>
}
else
{
    <h3 class="text-center">You have no yachts yet!</h3>
}